    <avalancheProblem>
      <problemType>wind_slab</problemType>
      <elevation uom="m">
        <lowerBound>2600</lowerBound>
      </elevation>
      <aspect>NE</aspect>
      <aspect>E</aspect>
      <aspect>N</aspect>

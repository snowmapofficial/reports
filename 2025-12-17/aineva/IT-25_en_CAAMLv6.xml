      <mainValue>low</mainValue>
      <validTimePeriod>all_day</validTimePeriod>
    </dangerRating>
    <avalancheProblem>
      <problemType>persistent_weak_layers</problemType>
      <elevation uom="m">
        <lowerBound>2400</lowerBound>
      </elevation>
      <aspect>NE</aspect>
      <aspect>NW</aspect>
      <aspect>N</aspect>
      <aspect>E</aspect>

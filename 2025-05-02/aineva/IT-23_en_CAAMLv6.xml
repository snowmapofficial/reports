    <avalancheProblem>
      <problemType>wet_snow</problemType>
      <elevation uom="m"/>
      <aspect>N</aspect>
      <aspect>W</aspect>
      <aspect>NW</aspect>
      <aspect>E</aspect>
      <aspect>NE</aspect>

      <elevation uom="m">
        <lowerBound>2200</lowerBound>
      </elevation>
      <aspect>E</aspect>
      <aspect>NE</aspect>
      <aspect>NW</aspect>
      <aspect>N</aspect>
      <aspect>W</aspect>

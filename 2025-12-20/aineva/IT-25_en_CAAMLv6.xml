    <dangerRating>
      <mainValue>low</mainValue>
      <validTimePeriod>all_day</validTimePeriod>
    </dangerRating>
    <avalancheProblem>
      <problemType>persistent_weak_layers</problemType>
      <elevation uom="m">
        <lowerBound>2300</lowerBound>
      </elevation>
      <aspect>N</aspect>
      <aspect>E</aspect>
      <aspect>W</aspect>
      <aspect>NE</aspect>
      <aspect>NW</aspect>

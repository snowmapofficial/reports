    <dangerRating>
      <mainValue>moderate</mainValue>
      <elevation uom="m">
        <lowerBound>2400</lowerBound>
      </elevation>
      <validTimePeriod>all_day</validTimePeriod>
    </dangerRating>
    <avalancheProblem>
      <problemType>wind_slab</problemType>
      <elevation uom="m">
        <lowerBound>2400</lowerBound>
      </elevation>
      <aspect>E</aspect>
      <aspect>W</aspect>
      <aspect>N</aspect>
      <aspect>NW</aspect>
      <aspect>NE</aspect>
      <aspect>SW</aspect>
      <aspect>S</aspect>
      <aspect>SE</aspect>
